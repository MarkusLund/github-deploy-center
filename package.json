{
  "name": "github-deploy-center",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@stitches/react": "^0.0.2",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "graphql": "^15.3.0",
    "graphql-request": "^3.2.0",
    "graphql-tag": "^2.11.0",
    "lodash-es": "^4.17.15",
    "overmind": "^25.0.2",
    "overmind-react": "^26.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^2.25.2",
    "react-scripts": "4.0.0",
    "typescript": "~4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "codegen": "graphql-codegen --config codegen.yml",
    "update-github-schema": "curl -H \"Authorization: Bearer $GITHUB_PAT\" https://api.github.com/graphql --output github.schema.json && json-format github.schema.json --indent 2",
    "package": "mkdir -p artifacts && cd build && zip -r ../artifacts/release.zip ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "defaults",
      "not IE 11",
      "not IE_Mob 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.18.0",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-graphql-request": "^2.0.2",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@types/lodash-es": "^4.17.3",
    "@types/react-select": "^3.0.22",
    "json-format-cli": "^1.1.1",
    "type-fest": "^0.18.0"
  }
}
